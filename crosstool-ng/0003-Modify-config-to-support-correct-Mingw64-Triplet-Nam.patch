From f627a779fd39ea754350e2679fb1255997a4d2ff Mon Sep 17 00:00:00 2001
From: martell <martellmalone@gmail.com>
Date: Thu, 11 Sep 2014 20:21:40 +0100
Subject: [PATCH 3/3] Modify config to support correct Mingw64 Triplet Names

---
 scripts/config.guess | 5 ++++-
 scripts/config.sub   | 6 +++---
 2 files changed, 7 insertions(+), 4 deletions(-)

diff --git a/scripts/config.guess b/scripts/config.guess
index 1f5c50c..013b4c4 100755
--- a/scripts/config.guess
+++ b/scripts/config.guess
@@ -821,7 +821,10 @@ EOF
 	echo ${UNAME_MACHINE}-pc-cygwin
 	exit ;;
     *:MINGW64*:*)
-	echo ${UNAME_MACHINE}-pc-mingw64
+	echo x86_64-w64-mingw32
+	exit ;;
+    *:MINGW32*:*)
+	echo i686-w64-mingw32
 	exit ;;
     *:MINGW*:*)
 	echo ${UNAME_MACHINE}-pc-mingw32
diff --git a/scripts/config.sub b/scripts/config.sub
index d654d03..d123d08 100755
--- a/scripts/config.sub
+++ b/scripts/config.sub
@@ -796,11 +796,11 @@ case $basic_machine in
 		basic_machine=microblaze-xilinx
 		;;
 	mingw64)
-		basic_machine=x86_64-pc
-		os=-mingw64
+		basic_machine=x86_64-w64
+		os=-mingw32
 		;;
 	mingw32)
-		basic_machine=i686-pc
+		basic_machine=i686-w64
 		os=-mingw32
 		;;
 	mingw32ce)
-- 
