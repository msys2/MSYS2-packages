+static void
+set_pe_stack_heap (char *resname, char *comname)
+{
+  set_pe_value (resname);
+
+  if (*optarg == ',')
+    {
+      optarg++;
+      set_pe_value (comname);
+    }
+  else if (*optarg)
+    einfo (_("%P%F: strange hex info for PE parameter '%s'\n"), optarg);
+}
